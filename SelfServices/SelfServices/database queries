CREATE TABLE BILLPAYLOG 
(
  CUSTOMERID NVARCHAR2(20) 
, PAIDDATE NVARCHAR2(15) 
) 
LOGGING 
TABLESPACE SYSTEM 
PCTFREE 10 
PCTUSED 40 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  FREELISTS 1 
  FREELIST GROUPS 1 
  BUFFER_POOL DEFAULT 
) 
NOPARALLEL;

CREATE TABLE USERS 
(
  USERNAME NVARCHAR2(20) 
, PASSWORD NVARCHAR2(128) 
, CUSTOMERID NVARCHAR2(20) NOT NULL 
, SECURITYQUESTION NVARCHAR2(60) 
, SECURITYANSWER NVARCHAR2(60) 
, EMAIL NVARCHAR2(50) 
, CONSTRAINT PK_CUSTID PRIMARY KEY 
  (
    CUSTOMERID 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX PK_CUSTID ON USERS (CUSTOMERID ASC) 
      LOGGING 
      TABLESPACE SYSTEM 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        FREELISTS 1 
        FREELIST GROUPS 1 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSTEM 
PCTFREE 10 
PCTUSED 40 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  FREELISTS 1 
  FREELIST GROUPS 1 
  BUFFER_POOL DEFAULT 
) 
NOPARALLEL;

CREATE TABLE REPORTREPAIR 
(
  CUSTOMER_ID VARCHAR2(20 BYTE) NOT NULL 
, REPORT_ID NVARCHAR2(20) NOT NULL 
, REPORT_NAME VARCHAR2(50 BYTE) NOT NULL 
, DESCRIPTION VARCHAR2(100 BYTE) NOT NULL 
, RAISED_DATE DATE NOT NULL 
, STATUS VARCHAR2(20 BYTE) NOT NULL 
, PROVISION_DATE DATE 
, CONSTRAINT TABLE1_PK PRIMARY KEY 
  (
    REPORT_ID 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TABLE1_PK ON REPORTREPAIR (REPORT_ID ASC) 
      LOGGING 
      TABLESPACE SYSTEM 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        FREELISTS 1 
        FREELIST GROUPS 1 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSTEM 
PCTFREE 10 
PCTUSED 40 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
